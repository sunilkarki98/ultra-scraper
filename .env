# Server Config
PORT=3000
NODE_ENV=development
LOG_LEVEL=info

# Redis (for BullMQ)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Scraping Config
# Format: protocol://user:pass@ip:port or multiple separated by comma
HEADLESS=true
CONCURRENCY=3

# .env
# Format: protocol://user:pass@ip:port
# Separate multiple proxies with a comma (,)
PROXY_LIST=http://user1:pass1@192.168.1.1:8080,http://user2:pass2@192.168.1.2:8080